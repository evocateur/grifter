{
    "name": "grifter",
    "description": "YUI Gallery builder",
    "author": "Dav Glass <davglass@gmail.com>",
    "version": "0.0.0",
    "dependencies": {
        "ansi-color": "*",
        "gear": "~0.7.0",
        "gear-lib": "~0.8.0",
        "nopt": "*",
        "yui-lint": "~0.1.1",
        "jshint": "~0.9.0",
        "cpr": "~0.0.4",
        "mkdirp": "*",
        "rimraf": "*",
        "istanbul": "~0.1.8",
        "request": "*",
        "yuglify": "~0.1.0",
        "graceful-fs": "~1.2.0",
        "osenv": "*",
        "tar": "~0.1.13",
        "shifter": "*",
        "yogi": "*",
        "timethat": "*",
        "archiver": "~0.1.0",
        "walkdir": "~0.0.5",
        "cssproc": "~0.0.1"
